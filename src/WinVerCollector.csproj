    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <VersionPrefix>1.0.0</VersionPrefix>
    <Authors>overdrive1708</Authors>
    <Copyright>Copyright Â© 2022 overdrive1708</Copyright>
    <PackageProjectUrl>https://github.com/overdrive1708/WinVerCollector</PackageProjectUrl>
